<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified"
    targetNamespace="http://www.pijz.rs/zalba-cutanje" vc:minVersion="1.1"
    xmlns:vc="http://www.w3.org/2007/XMLSchema-versioning"
    xmlns:zc="http://www.pijz.rs/zalba-cutanje">
    <xs:element name="ZalbaCutanje">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="Predmet" type="xs:string"/>
                <xs:element name="Kome_je_upucena">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="Naziv" type="xs:string"/>
                            <xs:element name="Adresa_za_postu" type="zc:Adresa"/>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
                <xs:element name="Protiv_organa" nillable="false" type="xs:string"/>
                <xs:element name="Razlog" type="xs:string"/>
                <xs:element name="Datum_zahtevanja" type="xs:date"/>
                <xs:element name="Podaci_o_zahtevu" type="xs:string"/>
                <xs:element name="Podnosilac_zalbe">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="Osoba" type="zc:Osoba"/>
                            <xs:element name="Drugi_kontakt" type="xs:string"/>
                            <xs:element name="Potpis" type="xs:string"/>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
                <xs:element name="Mesto_podnosenja_zalbe">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="Grad" type="xs:string"/>
                            <xs:element name="Datum" type="xs:date"/>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:complexType name="Adresa">
        <xs:sequence>
            <xs:element name="Grad" type="xs:string"/>
            <xs:element name="Ulica" type="xs:string"/>
            <xs:element name="Broj_ulice" type="xs:int"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="Osoba">
        <xs:sequence>
            <xs:element name="Ime" type="xs:string"/>
            <xs:element name="Prezime" type="xs:string"/>
            <xs:element name="Adresa" type="zc:Adresa"/>
        </xs:sequence>
    </xs:complexType>
</xs:schema>
